!URxvt*font: xft:DejaVu Sans Mono:size=8:antialias=true:autohint=true
!URxvt*font: xft:xos4 Terminus:size=8:antialias=true:autohint=true
URxvt*font: xft:UbuntuMono-R:size=10:antialias=true:autohint=true
!URxvt*font: xft:JetBrains Mono NL:size=9:antialias=true:autohint=true

URxvt*scrollBar:	false
URxvt.title:		term
URxvt.cursorBlink:	false
URxvt.saveLines:	5000
URxvt.urlLauncher:	firefox

*background: #d0d0d0
*foreground: #000000
*cursorColor:  #1c2027

! black
*color0:       #192033
*color8:       #000000

! red
*color1:       #a62a3e
*color9:       #f04758

! green
*color2:       #38912b
*color10:      #20d420

! yellow
*color3:       #b27d12
*color11:      #ddb62b

! blue
*color4:       #355c9a
*color12:      #45a3e6

! magenta
*color5:       #7c4f9f
*color13:      #c953ef

! cyan
*color6:       #258f8f
*color14:      #60c6c8

! white
*color7:        #a6b7bf
*color15:      #ffffff
